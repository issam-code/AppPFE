<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/v"
    tools:context=".ajouter_prof">


    <TableLayout
        android:layout_width="393dp"
        android:layout_height="635dp"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/tbrow22"
            android:layout_width="wrap_content"
            android:layout_height="86dp"
            android:layout_weight="10"
            android:gravity="bottom"
            android:orientation="vertical"
            android:visibility="visible"
            tools:visibility="visible">


            <EditText
                android:id="@+id/insnomp"
                android:layout_width="250dp"
                android:layout_height="40dp"
                android:layout_marginStart="24dp"
                android:layout_marginEnd="24dp"
                android:background="#9FF3EBE9"
                android:ems="10"
                android:hint=" Nom prof"
                android:inputType="textPersonName"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.494"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.466" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/tbrow3"
            android:layout_width="wrap_content"
            android:layout_height="86dp"
            android:layout_weight="10"
            android:gravity="bottom"
            android:orientation="vertical"
            android:visibility="visible"
            tools:visibility="visible">


            <EditText
                android:id="@+id/incprnomp"
                android:layout_width="250dp"
                android:layout_height="40dp"
                android:layout_marginStart="24dp"
                android:layout_marginLeft="24dp"
                android:layout_marginEnd="24dp"
                android:layout_marginRight="24dp"
                android:background="#9FF3EBE9"
                android:ems="10"
                android:hint=" Prenom prof"
                android:inputType="textPersonName"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/tbrow4"
            android:layout_width="wrap_content"
            android:layout_height="86dp"
            android:layout_weight="10"
            android:gravity="bottom"
            android:orientation="vertical"
            android:visibility="visible"
            tools:visibility="visible">


            <EditText
                android:id="@+id/inscmailp"
                android:layout_width="250dp"
                android:layout_height="40dp"
                android:layout_marginStart="24dp"
                android:layout_marginLeft="24dp"
                android:layout_marginEnd="24dp"
                android:layout_marginRight="24dp"
                android:background="#9FF3EBE9"
                android:ems="10"
                android:hint=" E-mail prof"
                android:inputType="textPersonName"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.539"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.448" />
        </androidx.constraintlayout.widget.ConstraintLayout>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/tbrow12"
            android:layout_width="wrap_content"
            android:layout_height="86dp"
            android:layout_weight="10"
            android:gravity="bottom"
            android:orientation="vertical"
            android:visibility="visible"
            tools:visibility="visible">

            <EditText
                android:id="@+id/usrnamep"
                android:layout_width="250dp"
                android:layout_height="40dp"
                android:layout_marginStart="24dp"
                android:layout_marginLeft="24dp"
                android:layout_marginEnd="24dp"
                android:layout_marginRight="24dp"
                android:background="#9FF3EBE9"
                android:ems="10"
                android:hint="Username (sans '/' , '@' , '.' , '-' ...)"
                android:inputType="textPersonName"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.494"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/tbrow14"
            android:layout_width="wrap_content"
            android:layout_height="86dp"
            android:layout_weight="10"
            android:gravity="bottom"
            android:orientation="vertical"
            android:visibility="visible"
            tools:visibility="visible">

            <EditText
                android:id="@+id/inspassp"
                android:layout_width="250dp"
                android:layout_height="40dp"
                android:layout_marginStart="24dp"
                android:layout_marginLeft="24dp"
                android:layout_marginEnd="24dp"
                android:layout_marginRight="24dp"
                android:background="#9FF3EBE9"
                android:ems="10"
                android:hint="Mot de passe"
                android:inputType="textPassword"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.508"
                tools:ignore="NotSibling" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/tbrow17"
            android:layout_width="wrap_content"
            android:layout_height="171dp"
            android:layout_weight="10"
            android:gravity="bottom"
            android:orientation="vertical"
            android:visibility="visible"
            tools:visibility="visible">


            <ListView
                android:id="@+id/listgr"
                android:layout_width="284dp"
                android:layout_height="165dp"
                android:layout_weight="80"
                android:padding="@dimen/text_margin"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.492"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:ignore="MissingConstraints" />


        </androidx.constraintlayout.widget.ConstraintLayout>


        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/tbrow16"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="10"
            android:gravity="bottom"
            android:orientation="vertical"
            android:visibility="visible"
            tools:visibility="visible">


            <Button
                android:id="@+id/inscrp"
                android:layout_width="116dp"
                android:layout_height="36dp"
                android:layout_marginStart="32dp"
                android:layout_marginEnd="8dp"
                android:background="#B577BD46"
                android:onClick="inscripr"
                android:text="Inscrit"
                android:textColor="#080B1B"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.827"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:ignore="NotSibling,OnClick"
                android:layout_marginLeft="32dp"
                android:layout_marginRight="8dp" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </TableLayout>
</androidx.constraintlayout.widget.ConstraintLayout>