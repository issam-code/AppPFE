<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/im"
    tools:context=".home_adm">

    <!-- TODO: Update blank fragment layout -->
    <androidx.drawerlayout.widget.DrawerLayout
        android:id="@+id/dr2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#97DDD167"
        tools:context=".menu">

        <androidx.appcompat.widget.LinearLayoutCompat
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            />

        <TableLayout
            android:layout_width="336dp"
            android:layout_height="436dp"
            android:visibility="visible">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/tbrowa"
                android:layout_width="wrap_content"
                android:layout_height="2dp"
                android:layout_weight="10"
                android:gravity="bottom"
                android:orientation="vertical"
                android:visibility="visible"
                tools:visibility="visible">

                <TextView
                    android:id="@+id/txtadm"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="@style/TextAppearance.AppCompat.Body1"
                    android:textColor="#282D42"
                    android:textSize="18sp"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />
            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/tbrowb"
                android:layout_width="wrap_content"
                android:layout_height="3dp"
                android:layout_weight="10"
                android:gravity="bottom"
                android:orientation="vertical"
                android:visibility="visible"
                tools:ignore="DuplicateIds"
                tools:visibility="visible">

                <Button
                    android:id="@+id/budoc12"
                    android:layout_width="110dp"
                    android:layout_height="40dp"
                    android:background="@drawable/bt"
                    android:onClick="groups"
                    android:text="Groupes"
                    android:textColor="#F0EDEA"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.498"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.386"
                    tools:ignore="OnClick" />
            </androidx.constraintlayout.widget.ConstraintLayout>


            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/tbrowd"
                android:layout_width="wrap_content"
                android:layout_height="0dp"
                android:layout_weight="10"
                android:gravity="bottom"
                android:orientation="vertical"
                android:visibility="visible"
                tools:ignore="DuplicateIds"
                tools:visibility="visible">

                <Button
                    android:id="@+id/buutil1"
                    android:layout_width="120dp"
                    android:layout_height="40dp"
                    android:background="@drawable/bt"
                    android:onClick="utils"
                    android:text="Professeurs"
                    android:textColor="#F0EDEA"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.498"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.022"
                    tools:ignore="OnClick" />

            </androidx.constraintlayout.widget.ConstraintLayout>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="53dp" />
        </TableLayout>

    </androidx.drawerlayout.widget.DrawerLayout>

</FrameLayout>